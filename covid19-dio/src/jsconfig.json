{
    "compilerOptions":{
        "target":"es6",
        "baseUrl": "src",
        "jsx": "react",
        "paths": {
            "components:/*":[
                "./components/*"
            ],
            "containers:/*":[
                "./components/*"
            ],
            "assets:/*":[
                "./assets/*"
            ],
            "commons:/*":[
                "./commons/*"
            ]
        }
    },
    "exclude": [
        "node_module",
        "build"
    ]
}